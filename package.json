{
  "name": "st-contracts",
  "version": "1.0.0",
  "license": "BUSL-1.1",
  "dependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-verify": "^2.0.5",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.0.5",
    "@openzeppelin/merkle-tree": "^1.0.6",
    "@openzeppelin/upgrades-core": "^1.32.5",
    "dotenv": "^16.4.5",
    "solc": "0.8.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "3.0.0-beta.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "chai": "^4.4.1",
    "ethers": "^6.11.1",
    "hardhat": "^2.22.2",
    "hardhat-gas-reporter": "^2.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solidity-coverage": "^0.8.12",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "node": "hardhat node",
    "console": "hardhat console",
    "compile": "hardhat compile",
    "chz-spicy:deploy": "hardhat run scripts/chiliz/deploy-spicy.ts --network chz-spicy",
    "eth-mainnet:deploy": "hardhat run scripts/ethereum/deploy-mainnet.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v2": "hardhat run scripts/ethereum/upgrade-st-v2.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v3": "hardhat run scripts/ethereum/upgrade-st-v3.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v4": "hardhat run scripts/ethereum/upgrade-st-v4.ts --network eth-mainnet",
    "eth-mainnet:verify": "hardhat verify --network eth-mainnet",
    "el:eth-holesky:deploy": "hardhat run scripts/eigen-layer/deploy-eth-holesky.ts --network eth-holesky",
    "el:eth-holesky:configure": "hardhat run scripts/eigen-layer/configure-eth-holesky.ts --network eth-holesky",
    "el:eth-mainnet:deploy": "hardhat run scripts/eigen-layer/deploy-eth-mainnet.ts --network eth-mainnet",
    "el:eth-mainnet:configure": "hardhat run scripts/eigen-layer/configure-eth-mainnet.ts --network eth-mainnet",
    "el:eth-mainnet:check-roles": "hardhat run scripts/eigen-layer/check-roles.ts --network eth-mainnet",
    "el:op-sepolia:deploy": "hardhat run scripts/eigen-layer/deploy-op-sepolia.ts --network op-sepolia",
    "el:op-sepolia:configure": "hardhat run scripts/eigen-layer/configure-op-sepolia.ts --network op-sepolia",
    "el:op-mainnet:deploy": "hardhat run scripts/eigen-layer/deploy-op-mainnet.ts --network op-mainnet",
    "el:op-mainnet:configure": "hardhat run scripts/eigen-layer/configure-op-mainnet.ts --network op-mainnet",
    "el:op-mainnet:check-roles": "hardhat run scripts/eigen-layer/check-roles.ts --network op-mainnet",
    "localhost:deploy": "hardhat run scripts/ethereum/deploy.ts --network localhost",
    "test": "hardhat test --typecheck",
    "coverage": "hardhat coverage --typecheck",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "remix": "remixd -s ./contracts -u https://remix.ethereum.org --read-only",
    "dev": "pnpm node",
    "typechain": "hardhat typechain",
    "typechain:export": "hardhat typechain && cp -r ./typechain ../st-v1-interface/src/types/typechain",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
