{
  "name": "st-contracts",
  "version": "1.0.0",
  "license": "BUSL-1.1",
  "dependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-verify": "^2.0.5",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.0.5",
    "@openzeppelin/merkle-tree": "^1.0.6",
    "@openzeppelin/upgrades-core": "^1.32.5",
    "dotenv": "^16.4.5",
    "solc": "0.8.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "3.0.0-beta.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "chai": "^4.4.1",
    "ethers": "^6.11.1",
    "hardhat": "^2.22.1",
    "hardhat-gas-reporter": "^2.0.2",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solidity-coverage": "^0.8.11",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "node": "npx hardhat node",
    "console": "npx hardhat console",
    "compile": "npx hardhat compile",
    "localhost:deploy": "npx hardhat run scripts/ethereum/deploy.ts --network localhost",
    "eth-holesky:deploy": "npx hardhat run scripts/ethereum/deploy-holesky.ts --network eth-holesky",
    "eth-holesky:verify": "npx hardhat verify --network eth-holesky",
    "eth-mainnet:deploy": "npx hardhat run scripts/ethereum/deploy-mainnet.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v2": "npx hardhat run scripts/ethereum/upgrade-st-v2.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v3": "npx hardhat run scripts/ethereum/upgrade-st-v3.ts --network eth-mainnet",
    "eth-mainnet:upgrade:st-v4": "npx hardhat run scripts/ethereum/upgrade-st-v4.ts --network eth-mainnet",
    "eth-mainnet:verify": "npx hardhat verify --network eth-mainnet",
    "el:op-sepolia:deploy": "npx hardhat run scripts/eigen-layer/deploy-op-sepolia.ts --network op-sepolia",
    "el:eth-sepolia:deploy": "npx hardhat run scripts/eigen-layer/deploy-eth-sepolia.ts --network sepolia",
    "el:eth-sepolia:verify": "npx hardhat verify --network eth-sepolia",
    "test": "npx hardhat test --typecheck",
    "coverage": "npx hardhat coverage --typecheck",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "remix": "remixd -s ./contracts -u https://remix.ethereum.org --read-only",
    "dev": "pnpm node",
    "typechain": "npx hardhat typechain",
    "typechain:export": "npx hardhat typechain && cp -r ./typechain ../st-v1-interface/src/types/typechain",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
